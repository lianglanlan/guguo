page {
  background-color: #f5f5f5;
}

.page-body {
  color: #6c6d6c;
}

.overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 10;
}

.navbar-wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 1;
  background-color: #fff;
  box-sizing: border-box;
}

.navbar-wrap .navbar-content {
  position: relative;
  display: flex;
  align-items: center;
  height: 88rpx;
  justify-content: center;
}

.navbar-wrap .search {
  position: absolute;
  left: 0;
  top: 0;
  padding: 0 20rpx;
  height: 88rpx;
  width: 70rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 40rpx 43rpx;
}

.navbar-wrap .tit {
  font-size: 38rpx;
  font-weight: 500;
  color: #000;
}

.nomore {
  font-size: 28rpx;
  padding: 52rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #b8b8b8;
  line-height: 44rpx;
}

.nomore .loading {
  display: block;
  width: 32rpx;
  height: 32rpx;
  box-sizing: border-box;
  border-radius: 50%;
  border: 4rpx solid rgba(153, 153, 153, 0.2);
  border-top-color: #999;
  margin: 2rpx 10rpx 0 0;
  animation: loading .6s linear infinite;
}

@keyframes loading {
  100% {
    transform: rotate(360deg);
  }
}

.banner-image {
  display: block;
  width: 100%;
  background-color: #ccc;
}

.del {
  margin-left: 20rpx;
  color: #ccc;
  text-decoration: line-through;
  font-weight: normal;
  font-size: 24rpx;
}

.tool-wrap {
  flex: none;
  display: flex;
  align-items: center;
}

.tool-wrap .less {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  box-sizing: border-box;
  border: 1rpx solid #ccc;
  color: #ccc;
  text-align: center;
  line-height: 45rpx;
}

.tool-wrap .num {
  margin: 0 26rpx;
}

.add {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  color: #fff;
  background-color: #d6ab7e;
  text-align: center;
  line-height: 45rpx;
}

.btn-primary, .btn-plain, .btn-disabled {
  display: flex;
  align-items: center;
  justify-content: center;
}

.btn-primary {
  background-color: #d6ab7e;
  color: #fff;
}

.btn-primary.disabled {
  opacity: 0.4;
}

.btn-plain {
  border: 2rpx solid #d6ab7e;
  color: #d6ab7e;
}

.btn-disabled {
  border: 2rpx solid #ccc;
  color: #ccc;
}

.column {
  margin: 20rpx;
  background-color: #fff;
  border-radius: 8rpx;
}

.order-list .list-wrap {
  padding: 10rpx 0;
}

.order-list .item {
  display: flex;
  align-items: center;
  padding: 10rpx 20rpx;
}

.order-list .item image {
  flex: none;
  height: 96rpx;
  width: 96rpx;
  margin-right: 16rpx;
  border-radius: 8rpx;
}

.order-list .item .detail {
  flex: 1;
  margin-right: 16rpx;
}

.order-list .item .name {
  color: #222;
  font-size: 28rpx;
  line-height: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: normal;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  display: block;
  display: -webkit-box;
}

.order-list .item .num {
  flex: none;
  width: 120rpx;
  font-size: 24rpx;
}

.order-list .item .price {
  flex: none;
  width: 120rpx;
  color: #d6ab7e;
  font-size: 28rpx;
  text-align: right;
}

.order-list .total {
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  line-height: 74rpx;
  text-align: right;
  border-top: 2rpx solid #f5f5f5;
  font-size: 24rpx;
  color: #222;
}

.order-list .total .price {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  font-weight: bold;
  margin-left: 15rpx;
}

.column-tit {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
  color: #222;
  line-height: 80rpx;
  font-size: 26rpx;
  border-bottom: 1rpx solid #efefef;
}

checkbox .wx-checkbox-input {
  border-radius: 50%;
  width: 30rpx;
  height: 30rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border-color: #d6ab7e;
  background: #d6ab7e;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
}

radio .wx-radio-input {
  border-radius: 50%;
  width: 30rpx;
  height: 30rpx;
}

radio .wx-radio-input.wx-radio-input-checked {
  border-color: #d6ab7e !important;
  background: #d6ab7e !important;
}

radio .wx-radio-input.wx-radio-input-checked::before {
  border-radius: 50%;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
}

.empty-wrap {
  position: absolute;
  left: 0;
  bottom: 0;
  top: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color: #fff;
}

.empty-wrap image {
  margin-top: -100rpx;
  width: 230rpx;
  height: 230rpx;
}

.empty-wrap .note {
  margin-top: 40rpx;
  line-height: 36rpx;
  font-size: 26rpx;
}

.empty-wrap .btn-primary {
  margin-top: 80rpx;
  width: 360rpx;
  height: 80rpx;
  border-radius: 8rpx;
}
