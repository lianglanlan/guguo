page {
  background-color: #fff;
}

.page-body {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.form-wrap {
  width: 600rpx;
}

.title {
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
  margin: 32rpx 0;
}

.ipt {
  width: 100%;
  height: 96rpx;
  margin-bottom: 32rpx;
  font-size: 28rpx;
  padding-left: 20rpx;
  border: 2rpx solid #ddd;
  border-radius: 16rpx;
  color: #333;
  box-sizing: border-box;
}

.btn-primary {
  margin-top: 60rpx;
  width: 600rpx !important;
  padding: 0;
  height: 84rpx;
  line-height: 84rpx;
}

.note01 {
  margin-top: 60rpx;
}

.note {
  line-height: 40rpx;
  font-size: 28rpx;
}

.banner-img {
  width: 100%;
  height: 520rpx;
}
