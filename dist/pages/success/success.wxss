page {
  background-color: #f5f5f5;
}

.success-wrap {
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  font-size: 26rpx;
  text-align: center;
}

.back {
  position: absolute;
  left: 0;
  top: 50%;
  padding: 18PX 14PX;
  transform: translateY(-50%);
  margin-top: -2PX;
}

.back:before {
  content: '';
  display: block;
  width: 11PX;
  height: 11PX;
  border-top: 2PX solid #000;
  border-left: 2PX solid #000;
  transform: rotate(-45deg);
}
