page {
  background-color: #f5f5f5;
}

.num {
  text-align: right;
}

.total {
  display: flex;
  align-items: center;
}

.total .time {
  flex: 1;
  min-width: 0;
  color: #ccc;
  text-align: left;
}

.total-num {
  flex: none;
  margin-right: 20rpx;
}

.total-price-wrap {
  flex: none;
}

.btn-cancle {
  flex: none;
  margin-left: 20rpx;
  height: 50rpx;
  padding: 0 20rpx;
  border-radius: 60rpx;
  font-size: 20rpx;
  box-sizing: border-box;
}

.column-footer {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  height: 80rpx;
  padding: 0 20rpx;
  border-top: 1rpx solid #efefef;
}
