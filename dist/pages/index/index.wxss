page {
  background-color: #fff;
  height: 100%;
}

.page-body {
  height: 100%;
  display: flex;
  flex-direction: column;
}

swiper {
  flex: none;
  height: 350rpx;
}

.banner-img {
  display: block;
  width: 750rpx;
  height: 350rpx;
}

.banner-wrap {
  flex: none;
  margin: 20rpx 30rpx 20rpx;
  border-radius: 20rpx;
  overflow: hidden;
}

.banner-wrap image {
  display: block;
  width: 100%;
  height: 140rpx;
}

.column-wrap {
  flex: 1;
  display: flex;
  overflow: hidden;
  box-shadow: 0 -2rpx 6rpx rgba(0, 0, 0, 0.1);
}

.sidebar-wrap {
  flex: none;
  background-color: #f6f6f6;
  height: 100%;
}

.sidebar-wrap scroll-view {
  height: 100%;
}

.con-scroll-wrap {
  height: 100%;
  flex: 1;
  min-width: 0;
  margin-right: -20rpx;
}

.con-wrap {
  box-sizing: border-box;
  padding: 0 50rpx 0 20rpx;
}

.sidebar-item {
  position: relative;
  padding: 0 20rpx;
  line-height: 98rpx;
  font-size: 24rpx;
}

.sidebar-item.cur {
  color: #222;
  background-color: #fff;
}

.sidebar-item.cur:before {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  width: 8rpx;
  background-color: #d6ab7e;
}

.con-tit {
  padding: 20rpx 0 0;
  line-height: 44rpx;
  font-size: 24rpx;
  font-weight: bold;
  color: #222;
}

.item {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}

.item-img {
  flex: none;
  display: block;
  width: 190rpx;
  height: 190rpx;
  margin-right: 20rpx;
  border-radius: 8rpx;
  background-color: #ccc;
}

.item-detail {
  flex: 1;
  min-width: 0;
}

.item-detail .name {
  line-height: 50rpx;
  font-size: 30rpx;
  color: #222;
}

.item-detail .desc {
  margin-top: 10rpx;
  font-size: 22rpx;
  line-height: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: normal;
  white-space: nowrap;
  display: block;
}

.item-detail .price {
  position: relative;
  margin-top: 10rpx;
  height: 50rpx;
  color: #222;
  font-size: 32rpx;
  font-weight: bold;
  display: flex;
  align-items: center;
}

.item-detail .add {
  position: absolute;
  right: 0;
  top: 0;
}

.item-detail .add text {
  margin-top: -10rpx;
}

.solution-list-wrap {
  padding-top: 20rpx;
}

.label-wrap {
  display: flex;
  flex-wrap: wrap;
  min-height: 80rpx;
}

.label-wrap view {
  margin-right: 16rpx;
  padding: 0 8rpx;
  font-size: 20rpx;
  line-height: 36rpx;
  background-color: #f6f6f6;
  color: #b7b7b7;
  border-radius: 4rpx;
  margin-top: 6rpx;
  height: 36rpx;
}

.solution-item {
  position: relative;
  height: 130rpx;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
  padding: 15rpx;
  box-sizing: border-box;
  color: #fff;
  z-index: 0;
  overflow: hidden;
}

.solution-item .name {
  color: #fff;
  font-size: 34rpx;
}

.solution-item .more {
  position: absolute;
  right: 15rpx;
  bottom: 15rpx;
  font-size: 24rpx;
}

.solution-item .img {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: -2;
}

.solution-item:after {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: -1;
}

.search-wrap {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  z-index: 1;
}

.search-box-wrap {
  display: flex;
  padding: 15rpx 0 15rpx 40rpx;
  border-bottom: 2rpx solid #e7e7e7;
}

.search-box {
  flex: 1;
  display: flex;
  align-items: center;
  background-color: #f7f7f7;
  border-radius: 68rpx;
  height: 68rpx;
  padding: 0 20rpx 0 15rpx;
}

.search-box .icon-search {
  flex: none;
  width: 34rpx;
  height: 100%;
  background-image: url(data:image/png;base64,);
  background-position: center;
  background-repeat: no-repeat;
  background-size: auto 34rpx;
}

.search-box input {
  flex: 1;
  height: 100%;
  line-height: 68rpx;
  padding: 0 30rpx;
  color: #222;
}

.search-box .icon-delete {
  flex: none;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background-color: #adabae;
  color: #fff;
  text-align: center;
  line-height: 36rpx;
}

.ipt-placeholder {
  color: #777;
}

.cancle {
  flex: none;
  width: 130rpx;
  text-align: center;
  line-height: 68rpx;
}

.suggest-wrap {
  position: absolute;
  left: 0;
  top: 100rpx;
  bottom: 0;
  right: 0;
  background-color: #fff;
}

.suggest-item {
  display: flex;
  align-items: center;
  height: 120rpx;
  padding: 0 40rpx;
  font-size: 28rpx;
}

.suggest-item.hover {
  background-color: #f8f8f8;
}

.suggest-item image {
  width: 80rpx;
  height: 80rpx;
}

.suggest-item .name {
  line-height: 30rpx;
  flex: 1;
  min-width: 0;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: normal;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  display: block;
  display: -webkit-box;
  padding: 0 20rpx;
}

.suggest-item .price {
  color: #222;
}

.suggest-empty {
  padding: 0 40rpx;
  line-height: 120rpx;
  color: #777;
}

.history-wrap {
  padding: 0 40rpx;
}

.history-wrap .tit {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 34rpx 0 20rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  color: #222;
  font-weight: bold;
}

.history-wrap .delete {
  font-weight: normal;
  font-size: 26rpx;
  color: #ccc;
}

.history-wrap .list-wrap {
  display: flex;
  flex-wrap: wrap;
}

.history-wrap .item {
  flex: none;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #f7f7f7;
  border-radius: 60rpx;
  margin-right: 24rpx;
  margin-bottom: 24rpx;
  font-size: 26rpx;
  color: #69696c;
  padding: 0 24rpx;
}
